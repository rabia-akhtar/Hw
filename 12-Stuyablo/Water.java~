	setOpponent("Flint the Firebender");
	setA1name("water blast");
	setA2name("ice shield");
	setA3name("water aura");
	setA4name("tsunami");
	setO1name("fireball");
	setO2name("flame block");
	setO3name("fire focus");
	setO4name("fire rain");
    }
}
