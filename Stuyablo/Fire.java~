public class Fire extends Basechar{
    public Fire(){
        setElement("fire");
	setOpponent("Waverly the Waterbender");
	setA1name("fireball");
	setA2name("flame block");
	setA3name("fire focus");
	setA4name("fire rain");
	setO1name("water blast");
	setO2name("ice shield");
	setO3name("water aura");
	setO4name("tsunami");
    }
}
