public String nonStart(String a, String b) {
    return a.substring(1) + b.substring(1);
}
public String makeAbba(String a, String b) {
    return a + b + b +a;
}
public int diff21(int n) {
    int a = Math.abs(n);
    if (a > 21){
	return 2 * Math.abs(n-21);
    }
    else {
	return Math.abs(21-n);
    }
}
